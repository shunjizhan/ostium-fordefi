# Alchemy API key for Arbitrum One RPC
ALCHEMY_API_KEY=your_alchemy_api_key_here

# Fordefi MPC signer configuration
FORDEFI_JWT_TOKEN=your_jwt_token_here

# Path to P-256 private key PEM file (default: keys/pk.pem)
# FORDEFI_PRIVATE_KEY_PATH=keys/pk.pem

# Wallet address (optional, will auto-discover from Fordefi if not set)
# FORDEFI_ADDRESS=0x_your_fordefi_wallet_address
